-cache "home_index-#{Rails.env}-#{request.protocol}#{request.host_with_port}#{request.fullpath}-#{DEPLOY_VERSION}" do
  .landing_page_logo
    %p.text-center
      = image_tag "GI_large_logo_03.png"
    .container
      .row
        .col-md-4.col-md-offset-4
          = link_to "Login", login_url(protocol: ENV['PROTOCOL']), :class => "btn btn-block btn-success btn-lg col-lg-8"
      .row
        &nbsp;
      .row
        .col-md-4.col-md-offset-4
          = link_to "Register", register_course_url(protocol: ENV['PROTOCOL']), :class => "btn btn-block btn-primary btn-lg col-lg-8"
  /
    =DEPLOY_VERSION
